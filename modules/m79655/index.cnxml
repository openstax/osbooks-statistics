            </m:msup>
            <m:mo>+</m:mo><m:mo>⋯</m:mo><m:mo>+</m:mo><m:msup>
             <m:mrow>
              <m:mo stretchy="false">(</m:mo><m:mn>5.5</m:mn><m:mo> </m:mo><m:mo>−</m:mo><m:mo> </m:mo><m:mover accent="true">
               <m:mi>x</m:mi>
               <m:mo stretchy="true">¯</m:mo>
