              <m:mo stretchy="false">(</m:mo><m:mn>6.9</m:mn><m:mo> </m:mo><m:mo>−</m:mo><m:mo> </m:mo><m:mover accent="true">
               <m:mi>x</m:mi>
               <m:mo stretchy="true">¯</m:mo>
              </m:mover>
