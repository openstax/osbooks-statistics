    <m:mtd columnalign="left">
     <m:mtable columnalign="left">
      <m:mtr>
       <m:mtd>
        <m:mrow/>
       </m:mtd>
      </m:mtr>
      <m:mtr>
       <m:mtd>
        <m:mover accent="true">
         <m:mi>x</m:mi>
         <m:mo stretchy="true">¯</m:mo>
        </m:mover>
        <m:mo>=</m:mo><m:mfrac>
         <m:mrow>
          <m:mn>79</m:mn><m:mo> </m:mo><m:mo>+</m:mo><m:mo> </m:mo><m:mn>145</m:mn><m:mo> </m:mo><m:mo>+</m:mo><m:mo> </m:mo><m:mo>⋯</m:mo><m:mo>+</m:mo><m:mo> </m:mo><m:mn>139</m:mn><m:mo>+</m:mo><m:mo> </m:mo><m:mn>99</m:mn>
         </m:mrow>
         <m:mrow>
          <m:mn>20</m:mn>
         </m:mrow>
        </m:mfrac>
        <m:mo>=</m:mo><m:mn>127.45</m:mn>
       </m:mtd>
      </m:mtr>
      <m:mtr>
       <m:mtd>
        <m:mrow/>
       </m:mtd>
      </m:mtr>
     </m:mtable>

    </m:mtd>
   </m:mtr>
   <m:mtr columnalign="left">
    <m:mtd columnalign="left">
     <m:mrow>
      <m:mi>s</m:mi><m:mo>=</m:mo><m:menclose notation="longdiv">
       <m:mrow>
        <m:mfrac>
         <m:mrow>
          <m:msup>
           <m:mrow>
            <m:mo stretchy="false">(</m:mo><m:mn>79</m:mn><m:mo>−</m:mo><m:mover accent="true">
             <m:mi>x</m:mi>
             <m:mo stretchy="true">¯</m:mo>
            </m:mover>
            <m:mo stretchy="false">)</m:mo>
           </m:mrow>
           <m:mn>2</m:mn>
          </m:msup>
          <m:mo>+</m:mo><m:mo> </m:mo><m:msup>
           <m:mrow>
            <m:mo stretchy="false">(</m:mo><m:mn>145</m:mn><m:mo> </m:mo><m:mo>−</m:mo><m:mo> </m:mo><m:mover accent="true">
             <m:mi>x</m:mi>
             <m:mo stretchy="true">¯</m:mo>
            </m:mover>
            <m:mo stretchy="false">)</m:mo>
           </m:mrow>
           <m:mn>2</m:mn>
          </m:msup>
          <m:mo>+</m:mo><m:mo>⋯</m:mo><m:mo>+</m:mo><m:msup>
           <m:mrow>
            <m:mo stretchy="false">(</m:mo><m:mn>139</m:mn><m:mo> </m:mo><m:mo>−</m:mo><m:mo> </m:mo><m:mover accent="true">
             <m:mi>x</m:mi>
