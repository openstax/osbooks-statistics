    <solution id="eip-733">
<label/>
    <para id="eip-idm8029072">
    <m:math>
     <m:mrow>
      <m:mover accent="true">
       <m:mi>x</m:mi>
       <m:mo>¯</m:mo>
      </m:mover>
      <m:mo>=</m:mo><m:mn>0.940</m:mn>
     </m:mrow>
    </m:math></para>
    <para id="eip-idm18738416">
    <m:math>
     <m:mrow>
      <m:mo> </m:mo><m:mfrac>
       <m:mi>α</m:mi>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mfrac>
       <m:mrow>
        <m:mn>1</m:mn><m:mo>−</m:mo><m:mi>C</m:mi><m:mi>L</m:mi>
       </m:mrow>
       <m:mn>2</m:mn>
      </m:mfrac>
      <m:mo>=</m:mo><m:mfrac>
       <m:mrow>
